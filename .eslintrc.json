{
    "extends": "airbnb",
    "rules": {
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "implicit-arrow-linebreak": 0,
        "import/prefer-default-export": 0,
        "jsx-a11y/href-no-hash": [0],
        "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
    }
}